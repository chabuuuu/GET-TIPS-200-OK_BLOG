    <!-- Chèn một đống quảng cáo -->
    <% var title=page.title; if (is_archive()){ title=__('archives'); if (is_month()){ title +=': ' + page.year + '/' +
      page.month; } else if (is_year()){ title +=': ' + page.year; } } else if (is_category()){ title=__('category')
      + ': ' + page.category; } else if (is_tag()){ if (theme.hashtag_prefix_before_tags) { title='#' + page.tag; } else
      { title=__('tag') + ': ' + page.tag; } } else if (theme.search==true && page.type=='search' ) { title=__('search')
      } else if (page.type=='tags' ) { title=__('tags'); } else if (page.type=='categories' ) { title=__('categories');
      } %>
      <title>
        <% if (title){ %>
          <%= title %> - <% } %>
              <%= config.title %>
      </title>
      <% if (theme.favicon){ %>
        <%- favicon_tag(theme.favicon) %>
          <% } %>
            <% if (theme.manifest) { %>
              <link rel='manifest' href='<%= theme.manifest %>'>
              <% } %>

                <% if (theme.og_enabled) { %>
                  <% var og={}; og['title']=(title ? title + ' - ' : '' ) + config.title; og['type']=is_post()
                    ? 'article' : 'website' ; og['url']=url; og['image']=page.cover_image ? page.cover_image :
                    theme.og_image; if (is_post()) { og['article:published_time']=page.date.toISOString();
                    og['article:author']=config.author; } if (theme['og']) { for (const i in theme.og) {
                    og[i]=theme.og[i]; } } if (page['og']) { for (const i in page.og) { og[i]=page.og[i]; } } %>
                    <% for (const i in og) { %>
                      <meta property="og:<%= i %>" content="<%= og[i] %>" />
                      <% } %>
                      <meta name="description" content="<%= page.description ? page.description : 'GET TIPS 200 OK: Trang blog chia sẻ tất tần tật mọi thứ tips về Backend, Devops, System Design,...'  %>" />
                        <% } %>

